\end_layout

\begin_layout Plain Layout

void loop() {   
\end_layout

\begin_layout Plain Layout

  digitalWrite(led, HIGH); 
\end_layout

\begin_layout Plain Layout

  // turn the LED on (HIGH is the voltage level)   
\end_layout

\begin_layout Plain Layout

  delay(1000);               
\end_layout

\begin_layout Plain Layout

  // wait for a second   
\end_layout

\begin_layout Plain Layout

  digitalWrite(led, LOW);    
\end_layout

\begin_layout Plain Layout

  // turn the LED off by making the voltage LOW   
\end_layout

\begin_layout Plain Layout

  delay(1000);               
\end_layout

\begin_layout Plain Layout

  // wait for a second 
\end_layout

